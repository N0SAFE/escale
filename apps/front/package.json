{
    "name": "@escale/front",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npx tsx ./bin/dev.ts",
        "build": "npm run dr:build && npm run openapi && next build",
        "start": "npx tsx ./bin/start.ts",
        "lint": "next lint --fix",
        "format": "prettier --write .",
        "dr:build": "npx declarative-routing build",
        "dr:build:watch": "npx declarative-routing build --watch",
        "openapi": "npm run openapi:yaml && npm run openapi:html",
        "openapi:yaml": "tsx ./src/routes/openapi.ts",
        "openapi:html": "npx @redocly/cli build-docs openapi-docs.yml"
    },
